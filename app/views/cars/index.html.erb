<h1>Cars</h1>
<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Make</th>
      <th>Model</th>
      <th>Trim</th>
      <th>Count</th>
      <th>Dealership</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.year.year %></td>
        <td><%= car.make.name if car.make %></td>
        <td><%= car.model.name if car.model %></td>
        <td><%= car.trim %></td>
        <td><%= car.count %></td>
        <td><%= car.dealership.name if car.dealership %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Car', new_car_path %> | 
<%= link_to "Home", root_path %>
